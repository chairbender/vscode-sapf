{
  "name": "vscode-sapf",
  "displayName": "vscode-sapf",
  "description": "Support for the sapf language",
  "version": "0.0.1",
  "publisher": "chairbender",
  "homepage": "https://github.com/chairbender/vscode-sapf",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chairbender/vscode-sapf"
  },
  "activationEvents": [
    "workspaceContains:**/{*.sapf}"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "cp": "copyup src/commands/help/* out",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "languages": [
      {
        "id": "sapf",
        "aliases": [
          "sapf"
        ],
        "extensions": [
          ".sapf"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "dependencies": {
    "@vscode/windows-registry": "^1.1.0",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
      "@types/mocha": "^2.2.42",
      "@types/node": "^16.0.0",
      "@types/vscode": "^1.60.0",
      "copyfiles": "^2.4.1",
      "eslint": "^4.11.0",
      "tar": "^6.1.11",
      "typescript": "^4.4.3"
  }
}